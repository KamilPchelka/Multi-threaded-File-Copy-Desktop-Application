<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="602.0"
            xmlns="http://javafx.com/javafx/8.0.121">
    <bottom>
        <AnchorPane prefHeight="50.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button layoutX="14.0" layoutY="1.0" mnemonicParsing="false" onAction="#handleCopyButton"
                        prefHeight="30.0" prefWidth="70.0" text="Copy" AnchorPane.bottomAnchor="10.0"
                        AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="10.0"/>
                <Button fx:id="stopAllButton" layoutX="494.0" layoutY="9.0" mnemonicParsing="false"
                        onAction="#handleStopAllButton" prefHeight="30.0" prefWidth="70.0" text="Stop all"
                        AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="10.0"/>
            </children>
        </AnchorPane>
    </bottom>
    <center>
        <ScrollPane>
            <VBox fx:id="containerForFileCopyAnchorPanes" BorderPane.alignment="CENTER">
            </VBox>
        </ScrollPane>
    </center>
</BorderPane>
